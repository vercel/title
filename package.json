{
  "name": "title",
  "version": "3.5.3",
  "description": "Capitalize your titles properly",
  "repository": "vercel/title",
  "scripts": {
    "test": "ava",
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "keywords": [
    "title",
    "capitalize",
    "chicago",
    "manual",
    "of",
    "style"
  ],
  "bin": {
    "title": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "author": "leo",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.0",
    "chalk": "^5.0.0",
    "clipboardy": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "ava": "6.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.13.2"
}
